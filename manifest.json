{
    "manifest_version": 3,
    "name": "Gerador de CPF",
    "description": "Gerador r√°pido de CPF",
    "version": "1.0",
    "permissions": [
        "contextMenus",
        "scripting",
        "activeTab",
        "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["gerarCPF.js"]
      }
    ],
    "icons": {
      "16": "icones/icon16.png",
      "48": "icones/icon48.png",
      "128": "icones/icon128.png"
    },
    "action":
    {
      "default_popup": "popup.html"
    }
  }